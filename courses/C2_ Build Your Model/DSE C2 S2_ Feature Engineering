dom() * x\n","# x2 will be sqrt of x1 plus some error\n","def func2(x, err2):\n","  return x**.5 + (err2 * random.randint(-1,1) * random.random() * x)\n","# x3 will be 50/50 correlated an random\n","def func3(x):\n","  if x%2 == 0:\n","    return x * .1\n","  else:\n","    return random.random()\n","\n","\n","x1 = np.arange(20,100)\n","x2 = [func2(i, err2) for i in x1]\n","x3 = [func3(i) for i in x1]\n","x4 = [random.random() for i in x1]\n","x5 = [func1(i, err1) for i in x4]\n","\n","\n","# a third variable that doesn't correlate with much of anything\n","# x3 = [random.random() * 10 for i in range(len(x1))]\n","\n","# take a look\n","plt.plot(x1, x2)\n","plt.plot(x1, x3)\n","plt.plot(x1, x4)\n","plt.plot(x1, x5)"],"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["[<matplotlib.lines.Line2D at 0x7f565e32dad0>]"]},"metadata":{"tags":[]},"execution_count":29},{"output_type":"display_data","data":{"image/png":"\n","text/plain":["<Figure size 432x288 with 1 Axes>"]},"metadata":{"tags":[],"needs_background":"light"}}]},{"cell_type":"code","metadata":{"colab":{"base_uri":"https://localhost:8080/","height":197},"id":"E3vgJN_lc1M9","executionInfo":{"status":"ok","timestamp":1617119256172,"user_tz":300,"elapsed":10398,"user":{"displayName":"Wesley Beckner","photoUrl":"https://lh3.googleusercontent.com/a-/AOh14Gis2tewog0nYcz7REtNxkAs58_fKdVn5wvb3mXkPQ=s64","userId":"17051665784581118920"}},"outputId":"87af6b63-a569-4c1b-ca1b-871405105c5f"},"source":["colin = pd.DataFrame([x1,x2,x3,x4,x5]).T\n","colin.columns = ['x1','x2','x3','x4','x5']\n","colin.head()"],"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/html":["<div>\n","<style scoped>\n","    .dataframe tbody tr th:only-of-type {\n","        vertical-align: middle;\n","    }\n","\n","    .dataframe tbody tr th {\n","        vertical-align: top;\n","    }\n","\n","    .dataframe thead th {\n","        text-align: right;\n","    }\n","</style>\n","<table bor